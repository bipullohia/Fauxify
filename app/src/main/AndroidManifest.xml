<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bipul.fauxify">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GoogleSignIn">

        </activity>
        <activity android:name=".AddAddressInfo"></activity>
        <activity android:name=".AddressConfirmation"></activity>
        <activity android:name=".WelcomeActivity"></activity>
        <activity android:name=".MainActivity"></activity>
        <activity
            android:name=".RestaurantDetails"
            android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".CartActivity"></activity>
        <activity android:name=".AddAddressInCart"></activity>
        <activity android:name=".AddressConfirmationInCart"></activity>
        <activity android:name=".OrderConfirmed"></activity>
    </application>

</manifest>